#== thedaemon's aliases ==#
alias acme='9 acme -f "/usr/local/plan9/font/kurinto/kurinto.18.mono.font"'
alias blender='vblank_mode=0 blender &'
alias brutaldonserver='cd ~/src/brutaldon && pipenv run python ./manage.py runserver'
alias c='clear'
alias cbar='clockbar | lemonbar -B#9EEEEE -b -g240x32+2320 -f "Kurinto Mono Regular" &'
alias qc='cc `sdl2-config --libs --cflags` -Wall -Wno-parentheses -Wno-missing-braces -Wno-switch -Wno-comment -Wno-sign-compare -Wno-unknown-pragmas -Wno-empty-body -Wno-unused-value -Wno-array-bounds -Wno-gnu-designator -Wno-unneeded-internal-declaration -fsigned-char -fsigned-char -fcommon  -std=c99 $1'
alias cl='clear && ls -cG'
alias cls='clear && ufetch'
alias d='doas'
alias df='df -h'
alias edwood='edwood -f /lib/font/bit/kurinto/kurinto.18.mono.font'
alias evilrestart='killall -HUP evilwm'
alias ff='firefox &'
alias ffs='doas $(history -p !!)'
alias geminiupload='rsync -azP /home/thedaemon/projects/pages/gemini/ root@137.220.41.150:/var/gemini/'
alias grafx2='vblank_mode=0 /usr/local/bin/grafx2 &'
alias h='history'
alias hg='history | grep $1'
alias icat="kitten icat"
alias l='ls -cG'
alias la='ls -FAG'
alias lc='ls -cG'
alias ll='ls -laFoG'
alias ls='ls -G'
alias lu='du -sh * | sort -h'
alias links='links -g &'
alias kpi='killall picom'
alias o='xdg-open'
alias oekaki='uxnemu ~/src/oekaki/bin/oekaki.rom'
alias pf="ps -e | grep $1"
alias pkg='doas pkg'
alias pkglist=' pkg query %n-%v'
alias q='exit'
alias redo='doas $(history -p !!)'
alias restart='shutdown -r now'
alias resolution='xrandr -s 2560x1440'
alias sdf='drawterm -h 9p.sdf.org -a 9p.sdf.org -u thedaemon &'
alias startvnc='x11vnc -usepw -many -display :0'
alias stats='stats -W 200x200 -lmisCe &'
alias tar='tar -czvf $1'
alias term='9term -f "/usr/local/plan9/font/kurinto/kurinto.18.mono.font" -W 800x400 &'
alias vpn='doas wg-quick up wg0'
alias vpnstop='doas wg-quick down wg0'
alias touchoff='xsetwacom --set "Wacom Bamboo 16FG 6x8 Finger touch" Touch off'
alias rotatetablet='xsetwacom --set "Wacom Bamboo 16FG 6x8 Pen stylus" Rotate half'
alias webupload='rsync -azP /home/thedaemon/projects/pages/thedaemons.space/ root@137.220.41.150:/var/www/htdocs/thedaemons.space/'
alias webdownload='rsync -azP root@137.220.41.150:/var/www/htdocs/thedaemons.space/ /home/thedaemon/projects/pages/thedaemons.space/'
alias winwatch='9 winwatch -W 200x200 -f "/usr/local/plan9/font/kurinto/kurinto.18.mono.font" -e "^(winwatch|stats|catclock|xclock|lemonbar)$" &'
alias x='xinit ~/.xinitrc -- -nolisten tcp'
alias xkeyreset='killall -HUP xbindkeys'
alias ..='cd ..'
alias ....='cd ../..'
