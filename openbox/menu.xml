    </action>
  </item>
</menu>

<menu id="shutdown-menu" label="Shutdown">
	<item label="Reboot">
		<action name="Execute">
			<command>sudo /sbin/shutdown -r now</command>
		</action>
	</item>
	<item label="Shutdown">
		<action name="Execute">
			<command>sudo /sbin/shutdown -h now</command>
		</action>
	</item>
</menu>

